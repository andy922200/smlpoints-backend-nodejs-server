{
  "name": "smlpoints-backend-nodejs-server",
  "version": "1.0.0",
  "description": "smlpoints backend Node.js Server",
  "main": "app.js",
  "scripts": {
    "start": "cross-env NODE_ENV=production && pnpm build && node dist/app.js",
    "dev": "cross-env NODE_ENV=dev nodemon app.ts",
    "build": "tsc",
    "lint": "pnpm eslint && pnpm prettier",
    "eslint": "eslint \"./**/*.{ts,js,d.ts}\" --fix",
    "prettier": "prettier --write ."
